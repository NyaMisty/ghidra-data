//
// Intrinsics (note: taken from our winnt.h)
// FIXME: 64-bit
//
static __inline__ BOOLEAN
InterlockedBitTestAndSet(
  LONG volatile *Base,
  LONG Bit)
{
  return (_InterlockedOr(Base, 1 << Bit) >> Bit) & 1;
}
static __inline__ BOOLEAN
InterlockedBitTestAndReset(
  LONG volatile *Base,
  LONG Bit)
{
  return (_InterlockedAnd(Base, ~(1 << Bit)) >> Bit) & 1;
}
//#if !defined(_X86AMD64_) // FIXME: what's _X86AMD64_ used for?
extern __inline__ __attribute__((__always_inline__,__gnu_inline__))
LONG64
_InterlockedAdd64(
  LONG64 volatile *Addend,
  LONG64 Value)
{
  return _InterlockedExchangeAdd64(Addend, Value) + Value;
}
//#endif
